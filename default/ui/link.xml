<?xml version="1.0" encoding="UTF-8"?>
<themes>
	<!-- link window -->
	<theme name="link-status" ref="-defaults">
		<theme name="link-slot" ref="resizableframe">

			<theme name="label" ref="label">
				<param name="font"><font>alphabeta-border</font></param>
			</theme>
			<param name="background"><image>label2.background</image></param>
			<param name="minSize"><int>150</int></param>
			<theme name="is-leader" ref="-defaults" allowWildcard="true">
				<param name="background"><image>is-leader</image></param>
				<param name="border"><border>0</border></param>
				<param name="maxWidth"><int>16</int></param>
			</theme>
			<theme name="link-monster" ref="-defaults" allowWildcard="true">
				<param name="maxWidth"><int>36</int></param>
				<param name="minWidth"><int>36</int></param>
			</theme>
		</theme>
	</theme>
</themes>
