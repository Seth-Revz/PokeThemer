<?xml version="1.0" encoding="UTF-8"?>
<themes>
	<!-- Map Widget -->
	<theme name="town-map" ref="base-frame">
		<param name="border"><border>0</border></param>
		<theme name="dialoglayout" ref="dialoglayout">
			<param name="border"><border>36,0,0,0</border></param>
          <!-- invisible button used for switching maps / can be overriden here -->
          <theme name="switch-map" ref="-defaults">
            <param name="background">
              <image>none</image>
            </param>
          </theme>
          <theme name="townmap-cursor" ref="-defaults" allowWildcard="true">
            <param name="font">
              <font>alphabeta</font>
            </param>
            <param name="border">
              <border>0,0</border>
            </param>
            <param name="background">
              <image>none</image>
            </param>
            <param name="textAlignment">
              <enum type="alignment">CENTER</enum>
            </param>
          </theme>
          <theme name="townmap-swarm-indicator" ref="boxlayout" allowWildcard="true">
            <param name="border">
              <border>0,0</border>
            </param>
            <param name="background">
              <image>chatframe-black.background</image>
            </param>
            <param name="textAlignment">
              <enum type="alignment">CENTER</enum>
            </param>
            <theme name="townmap-label" ref="-defaults" allowWildcard="true">
              <param name="font">
                <font>mechabold</font>
              </param>
              <param name="border">
                <border>0</border>
              </param>
              <param name="textAlignment">
                <enum type="alignment">LEFT</enum>
              </param>
            </theme>
          </theme>
          <theme name="boxlayout" ref="boxlayout">
            <param name="border">
              <border>0,0,0,0</border>
            </param>
            <param name="alignment">
              <enum type="alignment">LEFT</enum>
            </param>
            <theme name="townmap-label" ref="-defaults" allowWildcard="true">
              <param name="font">
                <font>mechabold</font>
              </param>
              <param name="border">
                <border>0</border>
              </param>
              <param name="textAlignment">
                <enum type="alignment">LEFT</enum>
              </param>
            </theme>
          </theme>
        </theme>
    </theme>

	<theme name="breed-window" ref="base-frame">
		<param name="minWidth"><int>640</int></param>
		<param name="minHeight"><int>561</int></param>
		<param name="fadeDurationShow"><int>200</int></param>
		<param name="fadeDurationHide"><int>200</int></param>
		<param name="border"><border>40,15,10,20</border></param>
		<theme name="dialoglayout" ref="-defaults">
			<param name="defaultGap"><dimension>2,2</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>

			<param name="border"><border>0,15,10,5</border></param>
			<theme name="button" ref="button">
				<param name="minHeight"><int>60</int></param>
			</theme>
			<theme name="min-button" ref="button">
				<param name="minHeight"><int>0</int></param>
				<param name="border"><border>0</border></param>
			</theme>
			<theme name="iv-slot" ref="label">
				<param name="fadeDurationShow"><int>200</int></param>
				<param name="fadeDurationHide"><int>0</int></param>
				<param name="background"><image>ui-header.background</image></param>
				<param name="border"><border>2,4</border></param>
				<param name="minWidth"><int>80</int></param>
				<param name="minHeight"><int>20</int></param>
				<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
				<param name="font"><font>pb-dark</font></param>
			</theme>
			<theme name="iv-value" ref="label">
				<param name="fadeDurationShow"><int>200</int></param>
				<param name="fadeDurationHide"><int>0</int></param>
				<param name="background"><image>ui-inputbox.background</image></param>
				<param name="border"><border>2,4</border></param>
				<param name="minWidth"><int>60</int></param>
				<param name="minHeight"><int>20</int></param>
				<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
				<param name="font"><font>pb-dark</font></param>
			</theme>
			<theme name="iv-value-green" ref="iv-value">
				<param name="background"><image>ui-inputbox.background</image></param>
				<param name="font"><font>pb-dark-green</font></param>
			</theme>

			<theme name="held-item" ref="label">
				<param name="fadeDurationShow"><int>200</int></param>
				<param name="fadeDurationHide"><int>0</int></param>
				<param name="background"><image>none</image></param>
				<param name="border"><border>26,0,14,0</border></param>
				<param name="minWidth"><int>130</int></param>
				<param name="minHeight"><int>20</int></param>
				<param name="textAlignment"><enum type="alignment">BOTTOM</enum></param>
				<param name="font"><font>pb-dark</font></param>
			</theme>

			<theme name="shared-value" ref="label">
				<param name="fadeDurationShow"><int>200</int></param>
				<param name="fadeDurationHide"><int>0</int></param>
				<param name="background"><image>ui-header.background</image></param>
				<param name="border"><border>1,4</border></param>
				<param name="minWidth"><int>100</int></param>
				<param name="minHeight"><int>20</int></param>
				<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
				<param name="font"><font>pb-dark</font></param>
			</theme>
			<theme name="shared-value-green" ref="shared-value">
				<param name="font"><font>pb-dark-green</font></param>
			</theme>

			<theme name="monster-slot" ref="button">
				<param name="fadeDurationShow"><int>200</int></param>
				<param name="minWidth"><int>32</int></param>
				<param name="minHeight"><int>32</int></param>

				<theme name="slot-health-progressbar" ref="-defaults" allowWildcard="true">
					<param name=""><image>mi-hpbar-green.*</image></param>
					<param name="border"><border>background.border</border></param>
					<param name="minWidth"><int>100</int></param>
				</theme>
			</theme>
		</theme>
	</theme>

	<theme name="item-slot" ref="-defaults">
		<param name="background"><image>ui-button.background</image></param>
		<param name="minWidth"><int>34</int></param>
		<param name="minHeight"><int>34</int></param>
		<param name="textAlignment"><enum type="alignment">bottom</enum></param>
		<param name="border"><border>0,0,2,0</border></param>
		<theme name="slot-health-progressbar" ref="-defaults" allowWildcard="true">
			<param name=""><image>mi-hpbar-green.*</image></param>
			<param name="border"><border>background.border</border></param>
			<param name="minWidth"><int>100</int></param>
		</theme>
	</theme>

	<theme name="seed-plant-dialog" ref="base-frame">
		<param name="minWidth"><int>350</int></param>
		<theme name="dialoglayout" ref="-defaults" allowWildcard="true">
			<param name="background"><image>none</image></param>
			<param name="border"><border>40,8,5,8</border></param>
			<param name="defaultGap"><dimension>5,2</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
			<theme name="label" ref="label">
				<param name="fadeDurationShow"><int>200</int></param>
				<param name="fadeDurationHide"><int>0</int></param>
				<param name="minWidth"><int>150</int></param>
				<param name="font"><font>pb-dark</font></param>
				<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
			</theme>
		</theme>
	</theme>

	<theme name="high-score-window" ref="base-frame">
		<param name="minWidth"><int>770</int></param>
		<param name="minHeight"><int>400</int></param>
		<theme name="dialoglayout" ref="-defaults" allowWildcard="true">
			<param name="background"><image>none</image></param>
			<param name="border"><border>40,8,5,8</border></param>
			<param name="defaultGap"><dimension>1,1</dimension></param>
			<param name="smallGap"><dimension>2,2</dimension></param>
			<param name="mediumGap"><dimension>4,4</dimension></param>
			<param name="largeGap"><dimension>6,6</dimension></param>
			<param name="namedGaps"><map/></param>
			<theme name="label" ref="label">
				<param name="fadeDurationShow"><int>200</int></param>
				<param name="fadeDurationHide"><int>0</int></param>
				<param name="minWidth"><int>150</int></param>
				<param name="font"><font>pb-dark</font></param>
				<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
			</theme>
		</theme>
	</theme>

	<theme name="high-score-table" ref="table" allowWildcard="true">
		<param name="minWidth"><int>650</int></param>
		<param name="columnWidths">
			<map>
				<param name="0"><int>40</int></param>
				<param name="1"><int>font.EM * 20</int></param>
				<param name="2"><int>80</int></param>
				<param name="3"><int>200</int></param>
			</map>
		</param>
		<theme name="stringcellrenderer" ref="stringcellrenderer">
			<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
		</theme>
		<theme name="box-layout-row" ref="-defaults" allowWildcard="true">
			<param name="background"><image>ui-table-row.background</image></param>
			<param name="border"><border>0,0</border></param>
			<param name="spacing"><int>10</int></param>
			<param name="alignment"><enum type="alignment">RIGHT</enum></param>
			<theme name="label" ref="label">
				<param name="border"><border>0,0</border></param>
			</theme>
		</theme>
	</theme>

    <theme name="scoreboard-window" ref="base-frame">
      <param name="minWidth">
        <int>550</int>
      </param>
      <param name="minHeight">
        <int>360</int>
      </param>
      <theme name="dialoglayout" ref="-defaults" allowWildcard="true">
        <param name="background">
          <image>none</image>
        </param>
        <param name="border">
          <border>40,8,5,8</border>
        </param>
        <param name="defaultGap">
          <dimension>1,1</dimension>
        </param>
        <param name="smallGap">
          <dimension>2,2</dimension>
        </param>
        <param name="mediumGap">
          <dimension>4,4</dimension>
        </param>
        <param name="largeGap">
          <dimension>6,6</dimension>
        </param>
        <param name="namedGaps">
          <map/>
        </param>
        <theme name="label" ref="label">
          <param name="fadeDurationShow">
            <int>200</int>
          </param>
                <param name="fadeDurationHide"><int>0</int></param>
                <param name="minWidth"><int>150</int></param>
                <param name="font"><font>pb-dark</font></param>
                <param name="textAlignment"><enum type="alignment">CENTER</enum></param>
            </theme>
        </theme>
    </theme>

    <theme name="scoreboard-table" ref="table" allowWildcard="true">
        <param name="columnWidths">
          <map>
            <param name="0">
              <int>font.EM * 18</int>
            </param>
            <param name="1">
              <int>90</int>
            </param>
          </map>
        </param>
        <theme name="stringcellrenderer" ref="stringcellrenderer">
            <param name="textAlignment"><enum type="alignment">CENTER</enum></param>
        </theme>
        <theme name="box-layout-row" ref="-defaults" allowWildcard="true">
            <param name="background"><image>ui-table-row.background</image></param>
            <param name="border"><border>0,0</border></param>
            <param name="spacing"><int>10</int></param>
            <param name="alignment"><enum type="alignment">RIGHT</enum></param>
            <theme name="label" ref="label">
                <param name="border"><border>0,0</border></param>
            </theme>
        </theme>
    </theme>

    <theme name="donation-link-dialog" ref="base-frame">
		<param name="minWidth"><int>350</int></param>
		<param name="minHeight"><int>150</int></param>
		<theme name="dialoglayout" ref="-defaults" allowWildcard="true">
			<param name="background"><image>none</image></param>
			<param name="border"><border>35,0,0,0</border></param>
			<param name="defaultGap"><dimension>5,2</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
			<theme name="label" ref="label">
				<param name="fadeDurationShow"><int>200</int></param>
				<param name="fadeDurationHide"><int>0</int></param>
				<param name="minWidth"><int>150</int></param>
				<param name="minHeight"><int>45</int></param>
				<param name="font"><font>pb-dark</font></param>
				<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
			</theme>
		</theme>
	</theme>

    <theme name="setko-judge" ref="confirm-widget">
        <param name="background"><image>none</image></param>
    </theme>

    <theme name="setko-judge-dialog" ref="dialoglayout">
        <param name="background"><image>label-area.background</image></param>
        <param name="minWidth"><int>450</int></param>
        <param name="minHeight"><int>270</int></param>
        <theme name="label" ref="label">
            <param name="fadeDurationShow"><int>200</int></param>
            <param name="fadeDurationHide"><int>0</int></param>
            <param name="minWidth"><int>150</int></param>
            <param name="minHeight"><int>50</int></param>
            <param name="font"><font>mechabold</font></param>
            <param name="textAlignment"><enum type="alignment">CENTER</enum></param>
        </theme>
    </theme>
</themes>
