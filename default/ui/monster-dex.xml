      <param name="border">
        <border>0</border>
      </param>
      <param name="defaultGap">
        <dimension>5,5</dimension>
      </param>
      <param name="smallGap">
        <dimension>8,8</dimension>
      </param>
      <param name="mediumGap">
        <dimension>10,10</dimension>
      </param>
      <param name="largeGap">
        <dimension>15,15</dimension>
      </param>
      <param name="namedGaps">
        <map/>
      </param>
      <param name="minWidth">
        <int>450</int>
      </param>
      <param name="maxWidth">
        <int>450</int>
      </param>
      <param name="minHeight">
        <int>260</int>
      </param>
      <param name="spacing">
        <int>0</int>
      </param>
      <param name="alignment">
        <enum type="alignment">LEFT</enum>
      </param>

      <theme name="widget" ref="label" allowWildcard="true">
        <param name="background">
          <image>ui-header.background</image>
        </param>
        <param name="textAlignment">
          <enum type="alignment">RIGHT</enum>
        </param>
        <param name="border">
          <border>9,5,7,5</border>
        </param>
        <param name="minWidth">
          <int>150</int>
        </param>
        <param name="minHeight">
          <int>20</int>
        </param>
        <param name="font">
          <font>pb-dark</font>
        </param>

        <theme name="progressbar-hp" ref="-defaults" allowWildcard="true">
          <param name="">
            <image>mi-hpbar-green.*</image>
          </param>
        </theme>
			<theme name="progressbar-attack" ref="-defaults" allowWildcard="true">
				<param name=""><image>mi-hpbar.*</image></param>
			</theme>
			<theme name="progressbar-defense" ref="-defaults" allowWildcard="true">
				<param name=""><image>progressbar-orange.*</image></param>
			</theme>
			<theme name="progressbar-spatk" ref="-defaults" allowWildcard="true">
				<param name=""><image>mi-hpbar-violet.*</image></param>
			</theme>
			<theme name="progressbar-spdef" ref="-defaults" allowWildcard="true">
				<param name=""><image>mi-hpbar-orange.*</image></param>
			</theme>
			<theme name="progressbar-speed" ref="-defaults" allowWildcard="true">
				<param name=""><image>mi-xpbar.*</image></param>
			</theme>
			<theme name="progressbar-total" ref="-defaults" allowWildcard="true">
				<param name=""><image>progressbar-white.*</image></param>
			</theme>
        </theme>

      <theme name="label" ref="label" allowWildcard="true">
        <param name="background">
          <image>ui-header.background</image>
        </param>
        <param name="textAlignment">
          <enum type="alignment">CENTER</enum>
        </param>
        <param name="border">
          <border>5</border>
        </param>
        <param name="minWidth">
          <int>150</int>
        </param>
        <param name="minHeight">
          <int>20</int>
        </param>
        <param name="font">
          <font>pb-dark</font>
        </param>
      </theme>

      <theme name="boxlayout" ref="-defaults" allowWildcard="true">
