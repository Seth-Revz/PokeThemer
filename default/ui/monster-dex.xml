            <param name="minWidth">
              <int>150</int>
            </param>
            <param name="minHeight">
              <int>8</int>
            </param>
            <param name="maxHeight">
              <int>8</int>
            </param>
            <param name="textAlignment">
              <enum type="alignment">CENTER</enum>
