<?xml version="1.0" encoding="UTF-8"?>
<themes>
	<theme name="monsterdex-box" ref="-defaults">
		<param name="background"><image>inner-area</image></param>
		<param name="border"><border>15,15</border></param>
		<param name="defaultGap"><dimension>5,5</dimension></param>
		<param name="smallGap"><dimension>8,8</dimension></param>
		<param name="mediumGap"><dimension>10,10</dimension></param>
		<param name="largeGap"><dimension>15,15</dimension></param>
		<param name="namedGaps"><map/></param>
	</theme>

	<theme name="monsterdex-button" ref="-defaults" allowWildcard="true">
		<param name="font"><font>alphabeta</font></param>
		<param name="border"><border>1,3</border></param>
		<param name=""><image>ui-button.*</image></param>
		<param name="background"><image>ui-button.background-defaultdex</image></param>
		<param name="textAlignment"><enum type="alignment">BOTTOM</enum></param>
	</theme>
	<theme name="monsterdex-button-caught" ref="monsterdex-button" allowWildcard="true">
		<param name="background"><image>ui-button.background-blue</image></param>
	</theme>

    <theme name="monsterdex-button-caught-ot" ref="monsterdex-button" allowWildcard="true">
      <param name="background"><image>ui-button.background-green</image></param>
    </theme>
  
    <theme name="monsterdex-button-caught-alpha" ref="monsterdex-button" allowWildcard="true">
      <param name="background"><image>ui-button.background-red</image></param>
    </theme>

	<theme name="monsterdex-button-evo-tree" ref="-defaults" allowWildcard="true">
		<param name="font"><font>symbols-white</font></param>
		<param name="background"><image>none</image></param>
		<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
	</theme>

	<theme name="label-area-monsterdex" ref="-defaults">
