            <param name="minWidth">
              <int>20</int>
            </param>
            <param name="textAlignment">
              <enum type="alignment">CENTER</enum>
            </param>
          </theme>
          <theme name="scrollpane" ref="scrollpane" allowWildcard="true">
            <theme name="vscrollbar" ref="vscrollbar-white"/>
          </theme>
          <theme name="combobox" ref="combobox" allowWildcard="true">
            <param name="border">
              <border>10,1</border>
            </param>
          </theme>
        </theme>
		<param name="border"><border>5,10,8,5</border></param>
		<theme name="tabbedpane" ref="base-tabbed-pane">
			<param name="background"><image>ui-inner-frame-tabbed-dex.background</image></param>
			<param name="minWidth"><int>100</int></param>
			<param name="minHeight"><int>435</int></param>
			<param name="border"><border>22,0,0,0</border></param>
			<theme name="tabbox" ref="tabbox">
				<param name="spacing"><int>2</int></param>
				<param name="alignment"><enum type="alignment">FILL</enum></param>
			</theme>
			<theme name="container" ref="container">
				<param name="border"><border>10,9,8,5</border></param>
              <theme name="dialoglayout" ref="dialoglayout">
                <param name="minWidth">
                  <int>550</int>
                </param>
                <param name="border">
                  <border>10,0,0,5</border>
                </param>
                <param name="defaultGap">
                  <dimension>5,5</dimension>
                </param>
                <param name="smallGap">
                  <dimension>8,8</dimension>
                </param>
                <param name="mediumGap">
                  <dimension>10,10</dimension>
                </param>
                <param name="largeGap">
                  <dimension>15,15</dimension>
                </param>
                <param name="namedGaps">
                  <map/>
                </param>
                <theme name="scrollpane" ref="scrollpane" allowWildcard="true">
                  <theme name="vscrollbar" ref="vscrollbar-white"/>
                  <param name="minWidth">
                    <int>590</int>
                  </param>
                </theme>
              </theme>
              <theme name="scrollpane" ref="scrollpane" allowWildcard="true">
                <param name="defaultGap">
                  <dimension>5,5</dimension>
                </param>
              </theme>
            </theme>
			<param name="inputMap"><inputMapDef>
				<action name="nextTab">CTRL TAB</action>
				<action name="prevTab">CTRL SHIFT TAB</action>
			</inputMapDef></param>
		</theme>
	</theme>

