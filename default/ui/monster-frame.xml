<?xml version="1.0" encoding="UTF-8"?>
<themes>
	<!-- Monster Frame -->
	<theme name="monster-frame" ref="resizableframe">
		<param name="fadeDurationShow"><int>200</int></param>
		<param name="fadeDurationHide"><int>200</int></param>
		<param name="background"><image>mi-frame-grid</image></param>
		<param name="border"><border>4,5,0,0</border></param>

		<param name="maxWidth"><int>439</int></param>
		<param name="maxHeight"><int>340</int></param>
		<theme name="title" ref="-defaults">
			<param name="textAlignment"><enum type="alignment">center</enum></param>
			<param name="font"><font>title-font-black</font></param>
			<param name="border"><border>2,283,0,0</border></param>
		</theme>

		<param name="titleAreaTop"><int>5</int></param>
		<param name="titleAreaLeft"><int>11</int></param>
		<param name="titleAreaRight"><int>-30</int></param>
		<param name="titleAreaBottom"><int>30</int></param>

		<param name="closeButtonX"><int>-26</int></param>
		<param name="closeButtonY"><int>13</int></param>

		<!-- Used for Name Section -->
		<theme name="nameplate" ref="-defaults">
			<param name="minWidth"><int>340</int></param>
			<param name="minHeight"><int>34</int></param>
			<param name="maxHeight"><int>34</int></param>
			<param name="border"><border>3,3,6,3</border></param>
			<theme name="label" ref="-defaults">
				<param name="font"><font>alphabeta-border</font></param>
				<param name="textAlignment"><enum type="alignment">LEFT</enum></param>
			</theme>
			<theme name="label2" ref="-defaults">
				<param name="font"><font>title-font</font></param>
				<param name="border"><border>3</border></param>
				<param name="background"><image>mi-nameplate</image></param>
			</theme>
			<param name="defaultGap"><dimension>2,0</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
		</theme>

		<!-- Used for Item Held section -->
		<theme name="itemplate" ref="-defaults">
			<param name="background"><image>none</image></param>
			<param name="minHeight"><int>34</int></param>
			<param name="maxHeight"><int>34</int></param>
			<param name="border"><border>10,22</border></param>
			<theme name="label" ref="-defaults">
				<param name="font"><font>alphabeta</font></param>
				<param name="textAlignment"><enum type="alignment">LEFT</enum></param>
			</theme>
			<theme name="label2" ref="-defaults">
				<param name="font"><font>alphabeta</font></param>
				<param name="border"><border>0,10,1,0</border></param>
			</theme>
			<param name="defaultGap"><dimension>2,0</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
		</theme>

		<theme name="dialoglayout" ref="-defaults">
			<param name="border"><border>5</border></param>
			<param name="defaultGap"><dimension>5,5</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>

			<theme name="tabbedpane" ref="-defaults">
				<param name="minWidth"><int>245</int></param>
				<param name="background"><image>none</image></param>
				<param name="border"><border>0,0,0,0</border></param>
				<theme name="tabbox" ref="-defaults">
					<!-- Not used, look below for custom tab buttons -->
					<theme name="tabbutton" ref="togglebutton">
						<param name=""><image>ui-tab-button.*</image></param>
					</theme>
					<param name="border"><border>0,0,0,0</border></param>
					<param name="spacing"><int>1</int></param>
					<param name="alignment"><enum type="alignment">FILL</enum></param>
				</theme>
				<theme name="container" ref="-defaults">
					 <theme name="dialoglayout" ref="-defaults">
						<param name="minHeight"><int>190</int></param>
						<param name="border"><border>10,0,0,0</border></param>
						<param name="defaultGap"><dimension>5,5</dimension></param>
						<param name="smallGap"><dimension>8,8</dimension></param>
						<param name="mediumGap"><dimension>10,10</dimension></param>
						<param name="largeGap"><dimension>15,15</dimension></param>
						<param name="namedGaps"><map/></param>

						<theme name="label-monster-area" ref="-defaults">
							<param name="background"><image>none</image></param>
							<param name="border"><border>3,5</border></param>
							<param name="defaultGap"><dimension>5,5</dimension></param>
							<param name="smallGap"><dimension>8,8</dimension></param>
							<param name="mediumGap"><dimension>10,10</dimension></param>
							<param name="largeGap"><dimension>15,15</dimension></param>
							<param name="namedGaps"><map/></param>

							<theme name="button" ref="-defaults" allowWildcard="true">
								<param name="font"><font>alphabeta-small</font></param>
								<param name="border"><border>0,0,0,0</border></param>
								<param name=""><image>button-small.*</image></param>
								<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
							</theme>

							<theme name="label-monster-title" ref="label" allowWildcard="true">
								<param name="background"><image>mi-label-title</image></param>
								<param name="border"><border>0,14,2,4</border></param>
								<param name="minWidth"><int>100</int></param>
								<param name="font"><font>alphabeta-small</font></param>
							</theme>

							<!-- Used with higher boxes -->
							<theme name="label-monster-title-wide" ref="label" allowWildcard="true">
								<param name="background"><image>mi-label-title-inset</image></param>
								<param name="border"><border>0,15,0,5</border></param>
								<param name="minWidth"><int>100</int></param>
								<param name="font"><font>alphabeta-small</font></param>
							</theme>

							<theme name="label-monster-title-full" ref="label" allowWildcard="true">
								<param name="background"><image>mi-label-title</image></param>
								<param name="border"><border>0,15,0,5</border></param>
								<param name="minWidth"><int>230</int></param>
								<param name="font"><font>alphabeta-small</font></param>
							</theme>

                            <!-- Active Skills -->
							<theme name="label-skill-use-name" ref="button" allowWildcard="true">
								<param name=""><image>mi-skill-use.*</image></param>
								<param name="border"><border>0,0,1,40</border></param>
								<param name="minHeight"><int>42</int></param>
								<param name="maxHeight"><int>42</int></param>
								<param name="minWidth"><int>164</int></param>
								<param name="maxWidth"><int>164</int></param>
								<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
							</theme>
							<!-- Inactive Skill -->
							<theme name="label-skill-name" ref="button" allowWildcard="true">
								<param name="background"><image>mi-skill</image></param>
								<param name="border"><border>0,0,0,41</border></param>
								<param name="minHeight"><int>43</int></param>
								<param name="maxHeight"><int>43</int></param>
								<param name="minWidth"><int>164</int></param>
								<param name="maxWidth"><int>164</int></param>

								<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
							</theme>

							<theme name="label-monster-value" ref="label" allowWildcard="true">
								<param name="background"><image>mi-label-value</image></param>
								<param name="textAlignment"><enum type="alignment">left</enum></param>
								<param name="border"><border>4,5,5,25</border></param>
								<param name="maxWidth"><int>110</int></param>
								<param name="minWidth"><int>110</int></param>
								<param name="font"><font>alphabeta-small</font></param>
							</theme>

                            <theme name="label-hidden-ability" ref="label-monster-value">
                                <param name="font"><font>alphabeta-yellow</font></param>
                            </theme>

							<theme name="label-monster-value-stat-complete" ref="label" allowWildcard="true">
								<param name="background"><image>mi-label-value</image></param>
								<param name="textAlignment"><enum type="alignment">left</enum></param>
								<param name="border"><border>4,5,5,25</border></param>
								<param name="maxWidth"><int>110</int></param>
								<param name="minWidth"><int>110</int></param>
								<param name="font"><font>alphabeta-lightgreen</font></param>
							</theme>

							<theme name="label-monster-value-stat-bad" ref="label" allowWildcard="true">
								<param name="background"><image>mi-label-value</image></param>
								<param name="textAlignment"><enum type="alignment">left</enum></param>
								<param name="border"><border>4,5,5,25</border></param>
								<param name="maxWidth"><int>110</int></param>
								<param name="minWidth"><int>110</int></param>
								<param name="font"><font>alphabeta-red</font></param>
							</theme>

							<theme name="label-monster-value-center" ref="label" allowWildcard="true">
								<param name="background"><image>mi-label-value</image></param>
								<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
								<param name="border"><border>15,25,15,25</border></param>
								<param name="maxWidth"><int>110</int></param>
								<param name="minWidth"><int>110</int></param>
								<param name="font"><font>alphabeta-small</font></param>
							</theme>

							<theme name="label-monster-value-tooltip" ref="label-monster-value" allowWildcard="true">
								<param name="mouseCursor"><cursor>cursor.help</cursor></param>
							</theme>

							<theme name="label-monster-value3" ref="label" allowWildcard="true">
								<param name="background"><image>none</image></param>
								<param name="textAlignment"><enum type="alignment">left</enum></param>
								<param name="border"><border>1,0,0,5</border></param>
								<param name="font"><font>pb-dark</font></param>
							</theme>

							<theme name="label-monster-value-name" ref="label" allowWildcard="true">
								<param name="background"><image>mi-label-value-inset</image></param>
								<param name="textAlignment"><enum type="alignment">left</enum></param>
								<param name="border"><border>2,0,0,0</border></param>
								<param name="maxWidth"><int>92</int></param>
								<param name="minWidth"><int>92</int></param>
								<param name="font"><font>alphabeta</font></param>
							</theme>

							<theme name="label-skill-arrowdown" ref="label" allowWildcard="true">
								<param name="background"><image>none</image></param>
								<param name="textAlignment"><enum type="alignment">left</enum></param>
								<param name="border"><border>0,4,0,1</border></param>
								<param name="maxWidth"><int>104</int></param>
								<param name="minWidth"><int>104</int></param>
								<param name="font"><font>pb-dark</font></param>
							</theme>
							<theme name="label-skill-arrowup" ref="label" allowWildcard="true">
								<param name="background"><image>none</image></param>
								<param name="textAlignment"><enum type="alignment">left</enum></param>
								<param name="border"><border>0,4,0,1</border></param>
								<param name="maxWidth"><int>104</int></param>
								<param name="minWidth"><int>104</int></param>
								<param name="font"><font>pb-dark</font></param>
							</theme>
							<theme name="level-toggle" ref="button">
								<param name="background"><image>icon.refresh</image></param>
								<param name="maxWidth"><int>16</int></param>
								<param name="maxHeight"><int>16</int></param>
								<param name="border"><border>0</border></param>
							</theme>
							<theme name="label" ref="-defaults">
								<param name="font"><font>alphabeta</font></param>
								<param name="textAlignment"><enum type="alignment">LEFT</enum></param>
							</theme>
						</theme>

					</theme>
				</theme>
				<param name="inputMap"><inputMapDef>
					<action name="nextTab">CTRL TAB</action>
					<action name="prevTab">CTRL SHIFT TAB</action>
				</inputMapDef></param>
			</theme>
		</theme>
	</theme>

	<!-- Custom Tab section for Monster Frame -->
	<theme name="monster-frame-tab" ref="-defaults" allowWildcard="true">
		<param name="minWidth"><int>20</int></param>
		<param name="minHeight"><int>20</int></param>
	</theme>

	<theme name="monster-frame-tab-info" ref="monster-frame-tab" overwrite="true">
		 <param name=""><image>mi-tab1.*</image></param>
	</theme>
	<theme name="monster-frame-tab-attack" ref="monster-frame-tab" overwrite="true">
		<param name=""><image>mi-tab4.*</image></param>
	</theme>
	<theme name="monster-frame-tab-stats" ref="monster-frame-tab" overwrite="true">
		 <param name=""><image>mi-tab5.*</image></param>
	</theme>
	<theme name="monster-frame-tab-history" ref="monster-frame-tab" overwrite="true">
		 <param name=""><image>mi-tab2.*</image></param>
	</theme>
	<theme name="monster-frame-tab-stats2" ref="monster-frame-tab" overwrite="true">
		 <param name=""><image>mi-tab3.*</image></param>
	</theme>
	<theme name="monster-frame-tab-stats3" ref="monster-frame-tab" overwrite="true">
		 <param name=""><image>mi-tab6.*</image></param>
	</theme>
	<theme name="monster-frame-tab-ribbons" ref="monster-frame-tab" overwrite="true">
		 <param name=""><image>mi-tab7.*</image></param>
	</theme>
	<theme name="monster-frame-tab-stats4" ref="monster-frame-tab" overwrite="true">
		 <param name=""><image>mi-tab8.*</image></param>
	</theme>
	<theme name="monster-frame-tab-particles" ref="monster-frame-tab" overwrite="true">
		 <param name=""><image>mi-tab9.*</image></param>
	</theme>



	<!-- Progress Bars -->
	<theme name="monsterframe-xp-progressbar" ref="-defaults" allowWildcard="true">
		<param name=""><image>mi-xpbar.*</image></param>
		<param name="border"><border>0,0</border></param>
		<param name="minWidth"><int>50</int></param>
	</theme>

	<theme name="monsterframe-hp-progressbar" ref="-defaults" allowWildcard="true">
		<param name=""><image>mi-hpbar-green.*</image></param>
		<param name="border"><border>0,0,0,0</border></param>
		<param name="minWidth"><int>50</int></param>
	</theme>

	<theme name="monsterframe-hp-progressbar-orange" ref="-defaults" allowWildcard="true">
		<param name=""><image>mi-hpbar-orange.*</image></param>
		<param name="border"><border>0,0</border></param>
		<param name="minWidth"><int>50</int></param>
	</theme>

	<theme name="monsterframe-hp-progressbar-red" ref="-defaults" allowWildcard="true">
		<param name=""><image>mi-hpbar.*</image></param>
		<param name="border"><border>0,0</border></param>
		<param name="minWidth"><int>50</int></param>
	</theme>

	<theme name="monsterframe-happiness-progressbar" ref="-defaults" allowWildcard="true">
		<param name=""><image>mi-hpbar-violet.*</image></param>
		<param name="border"><border>0,0</border></param>
		<param name="minWidth"><int>50</int></param>
	</theme>

	<theme name="monsterframe-progressbar-contest-0" ref="-defaults" allowWildcard="true">
		<param name=""><image>mi-hpbar.*</image></param>
	</theme>
	<theme name="monsterframe-progressbar-contest-1" ref="-defaults" allowWildcard="true">
		<param name=""><image>mi-xpbar.*</image></param>
	</theme>
	<theme name="monsterframe-progressbar-contest-2" ref="-defaults" allowWildcard="true">
		<param name=""><image>progressbar-pink.*</image></param>
	</theme>
	<theme name="monsterframe-progressbar-contest-3" ref="-defaults" allowWildcard="true">
		<param name=""><image>mi-hpbar-green.*</image></param>
	</theme>
	<theme name="monsterframe-progressbar-contest-4" ref="-defaults" allowWildcard="true">
		<param name=""><image>mi-hpbar-orange.*</image></param>
	</theme>
	<theme name="monsterframe-progressbar-contest-total" ref="-defaults" allowWildcard="true">
		<param name=""><image>progressbar-white.*</image></param>
	</theme>

	<theme name="monsterframe-ball" ref="button" allowWildcard="true">
		<param name="border"><border>0,0,0,0</border></param>
		<param name="background"><image>none</image></param>
	</theme>
</themes>
